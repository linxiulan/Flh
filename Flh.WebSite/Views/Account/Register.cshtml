@{
    Layout = "~/Views/Layout/_header.cshtml";
    ViewBag.Title = "注册";
}
<table class="common_table">
    <tr>
        <td>*手机：</td>
        <td><input name="Mobile" /></td>
    </tr>
    <tr>
        <td>验证码：</td>
        <td>
            <input name="VerifyCode" />
            <a href="#">获取验证码</a>
        </td>
    </tr>
    <tr>
        <td>*邮箱：</td>
        <td><input name="Email" /></td>
    </tr>
    <tr>
        <td>*密码：</td>
        <td><input name="Password" /></td>
    </tr>
    <tr>
        <td>确认密码：</td>
        <td><input name="ConfirmPwd" /></td>
    </tr>
    <tr>
        <td>*姓名：</td>
        <td><input name="Name" /></td>
    </tr>
    <tr>
        <td>*公司名称：</td>
        <td><input name="Company" /></td>
    </tr>
    <tr>
        <td>公司地址：</td>
        <td><input name="Address" /></td>
    </tr>
    <tr>
        <td>*从业人员：</td>
        <td>
            @Html.DropDownList("EmployeesCountRange", Util.GetEmployeesCountRangeSelectListItems(true, null))
        </td>
    </tr>
    <tr>
        <td>行业类别：</td>
        <td><input name="IndustryNo" /></td>
    </tr>
    <tr>
        <td>*采购权限：</td>
        <td>
            <input name="IsPurchaser" value="1" type="radio" />有
            &nbsp;
            <input name="IsPurchaser" value="0" type="radio" />无
        </td>
    </tr>
    <tr>
        <td>*是否需要发票：</td>
        <td>
            <input name="NeetInvoice" value="0" type="radio" />不需要
            &nbsp;
            <input name="NeetInvoice" value="1" type="radio" />需要
        </td>
    </tr>
    <tr>
        <td>电话：</td>
        <td><input name="Tel" /></td>
    </tr>
    <tr>
        <td>&nbsp;</td>
        <td><input value="注册" type="submit" /></td>
    </tr>
</table>
